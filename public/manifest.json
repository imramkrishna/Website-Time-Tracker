{
  "manifest_version": 3,
  "name": "Websites Time Tracker",
  "description": "This chrome extension tracks time spent on websites while using web browsers",
  "version": "1.0",
  "icons": {
    "32": "logo192.png",
    "48": "logo192.png",
    "16": "logo192.png",
    "128": "logo512.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo192.png",
      "32": "logo192.png",
      "48": "logo192.png",
      "128": "logo512.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  }
}